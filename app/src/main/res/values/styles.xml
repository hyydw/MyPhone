<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#2e8abb</item> <!--浅蓝色-->
        <item name="colorPrimaryDark">#7290e9</item> <!--深蓝色-->
    </style>
    <!--<item name="android:windowNoTitle">true</item>-->
    <style name="PhotoSelectDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/Anim_scale</item>
        <item name="android:inAnimation">@anim/bottomin</item>
        <item name="android:outAnimation">@anim/bottomout</item>
    </style>
    <style name="Anim_scale" parent="@style/Animation.AppCompat.Dialog">
        <item name="android:activityOpenEnterAnimation">@anim/bottomin</item>
        <item name="android:activityOpenExitAnimation">@anim/bottomout</item>
        <item name="android:activityCloseEnterAnimation">@anim/bottomin</item>
        <item name="android:activityCloseExitAnimation">@anim/bottomout</item>

    </style>
